    <wiki:id xmlns:wiki="http://exist-db.org/xquery/wiki">MarkupHelp</wiki:id>
    <atom:published>2011-11-13T22:31:37.365+01:00</atom:published>
    <atom:author>
        <atom:name>admin</atom:name>
    </atom:author>
    <atom:title>Wiki Markup Help</atom:title>
    <atom:content type="xhtml">
        <div xmlns="http://www.w3.org/1999/xhtml">
            <h2>Headings</h2>
            <p>
                <code>=Heading1=</code>
            </p>
            <p>
                <code>==Heading2==</code>
            </p>
            <p>
                <code>===Heading3===</code>
            </p>
            <h2>Inline</h2>
            <p>
                <code>**</code>
                <span class="strong">strong</span>
                <code>**</code>
            </p>
            <p>
                <code>__</code>
                <em>emphasis</em>
                <code>__</code>
            </p>
            <p>
                <code>$$</code>
                <code>code</code>
                <code>$$</code>
            </p>
            <p>
                <code>^^</code>
                <sup>superscript</sup>
                <code>^^</code>
            </p>
            <p>
                <code>~~</code>
                <sub>subscript</sub>
                <code>~~</code>
            </p>
            <h2>Blocks</h2>
            <p>
                <code>* unordered list item</code>
            </p>
            <p>
                <code>+ numbered list item</code>
            </p>
            <p>
                <code>{{{verbatim text}}}</code>
            </p>
            <p>
                <code>|| table || cells ||</code>
            </p>
            <p>
                <code>-- horizontal line</code>
            </p>
            <p>
                <code>Q: quoted block</code>
            </p>
            <p>
                <code>&gt; a quoted line</code>
            </p>
            <p>
                <code>{{style='background-color: red'}}block</code>
            </p>
            <h2>Links</h2>
            <p>
                <code>http://external.link/</code>
            </p>
            <p>
                <code>[URL | label]</code>
            </p>
            <p>
                <code>[/internal/path | label]</code>
            </p>
            <h2>Macros</h2>
            <p>
                <code>$image(src=path float=left|right)</code>
            </p>
            <p>
                <code>{code lang="XQuery"} code block {/code}</code>
            </p>
            <p>
                <code>{script} inline XQuery block {/script}</code>
            </p>
        </div>
    </atom:content>
</atom:entry>